
public class get_you_a_cat {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Verse1();
		Verse2();
		Verse3();
		Verse4();
		Verse5();
		Verse6();
		
	}
	public static void Cat () {
		System.out.println("Brough me a cat and the cat please me,");
		System.out.println("I fed my cat under yonder tree.");
		
	}
	public static void CatGoes() {
		System.out.print("Cat goes fiddle-i-fee.");
		System.out.println("");
	}
	public static void Verse1(){
		Cat();
		CatGoes();
		System.out.println("");
		}
	public static void HenGoes() {
		System.out.println("Hen goes chimmy-chuck, chimmy chuck,");
		
	}
	public static void hen() {
		System.out.println("Brought me a hen and the hen please me,");
		System.out.println("I fed my hen under yonder tree.");
		HenGoes();
		}
	public static void Verse2() {
		hen();
		CatGoes();
		System.out.println("");
	}
	
	public static void DuckGoes() {
		System.out.println("Duck goes quack, quack,");
	}
	public static void Verse3() {
		System.out.println("Brought me a duck and the duck please me");
		System.out.println("I fed my duck under yonder tree.");
		DuckGoes();
		HenGoes();
		CatGoes();
		System.out.println("");
	}
	
	public static void GooseGoes() {
		System.out.println("The Goose goes hissy, hissy,");
	}
	
	public static void Verse4 () {
		System.out.println("Bought me a goose and the goose please me,");
		System.out.println("I fed my goose under yonder tree.");
		GooseGoes();o
		DuckGoes();
		HenGoes();
		CatGoes();
		System.out.println("");
	}
	public static void SheepGoes() {
		System.out.println("Sheep goes baa, baa,");
	}
	public static void Verse5() {
		System.out.println("Bought me a sheep and the sheep pleased me,");
		System.out.println("I fed my sheep under yonder tree.");
		SheepGoes();
		GooseGoes();
		DuckGoes();
		HenGoes();
		CatGoes();
		System.out.println("");
		
	}
	public static void PestoGoes() {
		System.out.println("Pesto goes meow");
	}
	public static void Verse6() {
		System.out.println("I got my own kitty and I love the kitty");
		System.out.println("I feed my kitty a bunch of treats and she loves me");
		PestoGoes();
		SheepGoes();
		GooseGoes();
		DuckGoes();
		HenGoes();
		CatGoes();
		System.out.println("");
	}
	}
	
	
	
	
		


